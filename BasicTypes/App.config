<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.diagnostics>
    <switches>
      <add name="stringify" value="Information"/>
    </switches>
    <sources>
      <source name="stringify" switchName="stringify">
        <listeners>
          <clear/>
          <add name="console"/>
        </listeners>
      </source>
      <source name="parser" switchValue="Information">
        <listeners>
          <clear/>
          <add name="console"/>
        </listeners>
      </source>
    </sources>
    <sharedListeners>
      <add name="console" type="System.Diagnostics.ConsoleTraceListener" />
    </sharedListeners>
  </system.diagnostics>
</configuration>